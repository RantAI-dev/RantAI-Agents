{
  "name": "rantai-agents",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.21.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "esbuild",
      "canvas"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "build": "next build",
    "dev": "tsx server.ts",
    "dev:next": "next dev",
    "dev:https:setup": "mkdir -p certs && cd certs && mkcert localhost",
    "lint": "eslint .",
    "start": "NODE_ENV=production tsx server.ts",
    "db:seed": "pnpm tsx prisma/seed.ts",
    "db:migrate": "pnpm prisma migrate dev",
    "db:studio": "pnpm prisma studio",
    "db:push": "pnpm prisma db push",
    "db:generate": "pnpm prisma generate",
    "rag:ingest": "pnpm tsx scripts/ingest-knowledge-base.ts",
    "rag:test": "pnpm tsx scripts/test-rag.ts",
    "test:models": "pnpm tsx scripts/test-models.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "db:cleanup": "pnpm tsx scripts/cleanup-conversations.ts",
    "setup": "pnpm docker:up && sleep 5 && pnpm db:push && pnpm db:generate && pnpm db:seed",
    "migrate:s3": "pnpm tsx scripts/migrate-files-to-s3.ts",
    "migrate:s3:dry-run": "pnpm tsx scripts/migrate-files-to-s3.ts --dry-run"
